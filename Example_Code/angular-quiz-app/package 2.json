  },
  "devDependencies": {
    "@types/core-js": "^2.5.1",
    "@types/hammerjs": "^2.0.36",
    "@types/iscroll": "^5.2.2",
    "@types/materialize-css": "^0.100.0",
    "ansi-colors": "^3.2.4",
    "del": "^4.1.1",
    "fancy-log": "^1.3.2",
    "flat-color-icons": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-filter": "^6.0.0",
    "gulp-if": "^2.0.0",
    "gulp-less": "^4.0.1",
    "gulp-preprocess": "^3.0.2",

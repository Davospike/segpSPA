        "jspmNodeConversion": true,
        "map": {}
      },
      "npm:safe-buffer@5.1.2": {
        "browser": "index.js"
      },
      "npm:tslib@1.10.0": {
        "format": "cjs",
        "meta": {
          "tslib.js": {
            "esModule": true
          }
        }
      }
    }
  }
